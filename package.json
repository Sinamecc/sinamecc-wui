{
  "name": "sinamecc-wui",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "= 16.13.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build",
    "start": "npm run env -s && ng serve --configuration=local --proxy-config proxy.conf.js --port 4200",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "eslint \"src/**/*.ts\" --fix && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && eslint \"src/**/*.ts\" --fix && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^18.2.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/material": "^17.3.10",
    "@angular/material-moment-adapter": "^17.3.10",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/router": "^18.2.12",
    "@angular/service-worker": "^18.2.12",
    "@ngx-translate/core": "^14.0.0",
    "@types/moment": "^2.13.0",
    "angulartics2": "^12.2.1",
    "core-js": "^3.39.0",
    "lodash": "4.17.21",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^6.7.0",
    "moment": "2.29",
    "or": "^0.2.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.12",
    "@angular/language-service": "^18.2.12",
    "@angularclass/hmr": "^3.0.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@ngx-rocket/scripts": "^5.2.3",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/lodash": "4.14.202",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "globals": "^15.12.0",
    "hads": "^2.0.1",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "pretty-quick": "^4.0.0",
    "protractor": "~7.0.0",
    "puppeteer": "^2.1.1",
    "stylelint": "~14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "~8.0.0",
    "stylelint-config-standard": "~29.0.0",
    "stylelint-scss": "~5.0.0",
    "tailwindcss": "^3.4.15",
    "ts-node": "^8.8.1",
    "typescript": "~5.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}